{
    "name": "@beep/monorepo",
    "repository": "git@gitlab.nussman.us:beep-app/Beep.git",
    "author": "Banks Nussman",
    "workspaces": [
        "./*"
    ],
    "scripts": {
        "install:all": "yarn install --frozen-lockfile",
        "build:api": "yarn workspace @beep/api build",
        "build": "yarn build:api && yarn workspace @beep/api build",
        "up": "yarn install:all && yarn build:api && yarn start:all",
        "start:all": "concurrently \"yarn workspace @beep/app start\" \"yarn workspace @beep/api dev\" \"yarn workspace @beep/website start\"",
        "codegen": "concurrently \"yarn workspace @beep/app codegen\" \"yarn workspace @beep/website codegen\""
    },
    "license": "SEE LICENSE IN LICENSE.MD",
    "private": true,
    "dependencies": {
        "concurrently": "^6.0.0"
    },
    "devDependencies": {
        "expo-yarn-workspaces": "^1.3.1"
    },
    "resolutions": {
        "react": "^0.0.0-experimental-33c3af284",
        "react-dom": "^0.0.0-experimental-33c3af284"
    }
}
